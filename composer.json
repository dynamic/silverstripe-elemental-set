{
  "name": "dynamic/silverstripe-elemental-set",
  "description": "A set of Elements to display",
  "authors": [
    {
      "name": "Dynamic",
      "email": "dev@dynamicagency.com",
      "homepage": "http://www.dynamicagency.com"
    }
  ],
  "keywords": [
    "silverstripe", "elemental", "elemental set"
  ],
  "type": "silverstripe-vendormodule",
  "license": "BSD-3-Clause",
  "require": {
    "dnadesign/silverstripe-elemental": "^2.0 || ^3.0 || ^4.0",
    "silverstripe/recipe-cms": "^1.0 || ^4.0",
    "silverstripe/vendor-plugin": "^1.0"
  },
  "require-dev": {
    "phpunit/PHPUnit": "^5.7",
    "squizlabs/php_codesniffer": "*"
  },
  "config": {
    "process-timeout": 600
  },
  "autoload": {
    "psr-4": {
      "Dynamic\\ElementalSets\\": "src/",
      "Dynamic\\ElementalSets\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
